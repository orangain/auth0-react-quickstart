{
  "locales": [
    "en",
    "ja"
  ],
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ],
      "exclude": [
        "**/node_modules/**",
        "**/*.d.ts"
      ]
    }
  ],
  "format": "po",
  "compileNamespace": "ts"
}
